{
  "name": "visited-ukraine-map",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm:dev --workspace backend\" \"npm:dev --workspace frontend\"",
    "build": "npm --workspace frontend run build",
    "test": "npm --workspace frontend run test && npm --workspace backend run test",
    "e2e": "npm --workspace frontend run e2e",
    "backup": "npm --workspace backend run backup"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
